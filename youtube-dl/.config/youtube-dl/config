# save all movies to home directory
-o ~/video/youtube-dl/%(title)s.%(uploader)s.%(uploader_id)s.%(channel)s.%(channel_id)s.%(release_date)s.%(id)s.%(ext)s

--format best/bestvideo+bestaudio

#--yes-playlist

--restrict-filenames

--no-overwrites

--verbose

--console-title

#--newline

#--all-subs

#--sub-format srt

#--convert-subs srt

#--audio-quality 0
