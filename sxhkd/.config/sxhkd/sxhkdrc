# QMK MEH is equal to control + shift + alt
# QMK HYPER is equal to control + shift + alt + super

control + shift + alt + super + x
	pkill sxhkd && sxhkd &

super + Return
	$TERMINAL

control + shift + alt + super + space
	rofi -show combi &

control + shift + alt + super + l
	systemctl suspend
# vim:ft=config:
