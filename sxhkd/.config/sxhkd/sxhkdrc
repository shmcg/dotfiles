# QMK MEH is equal to control + shift + alt
# QMK HYPER is equal to control + shift + alt + super

# wm independent hotkeys

control + shift + alt + super + x
	pkill sxhkd && sxhkd &
	#pkill -USR1 -x sxhkd

super + Return
	$TERMINAL

super + space
	rofi -show combi &

control + shift + alt + super + l
	systemctl suspend

# vim:ft=config:
