! desert Xresources palette
#define black #7f7f8c
#define bright_gray #8a7f7f
#define desert_red #cd5c5c
#define bright_red #ff0000
#define desert_green #9acd32
#define bright_green #89fb98
#define desert_yellow #bdb76b
#define bright_yellow #f0e68c
#define desert_blue #75a0ff
#define bright_blue #6dceeb
#define desert_magenta #eeee00
#define bright_magenta #ffde9b
#define desert_cyan #cd853f
#define bright_cyan #ffa0a
#define white #666666
#define bright_white #c2bfa5
#define desert_foreground #ffffff
#define desert_background #333333
#define desert_current_line #6b8e24
#define desert_comment #4d4d4d

*.foreground: desert_foreground
*.background: desert_background

! black
*.color0:     black
*.color8:     bright_gray

! red
*.color1:     desert_red
*.color9:     bright_red

! green
*.color2:     desert_green
*.color10:    bright_green

! yellow
*.color3:     desert_yellow
*.color11:    bright_yellow

! blue
*.color4:     desert_blue
*.color12:    bright_blue

! magenta
*.color5:     desert_magenta
*.color13:    bright_magenta

! cyan
*.color6:     desert_cyan
*.color14:    bright_cyan

! white
*.color7:     white
*.color15:    bright_white
