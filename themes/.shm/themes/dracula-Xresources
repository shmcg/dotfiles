#define drac_pink #FF79C6
#define drac_cyan #8BE9FD
#define white #BFBFBF
#define bright_green #5AF78E
#define bright_yellow #F4F99D
#define bright_purple #CAA9FA
#define bright_pink #FF92D0
#define bright_cyan #9AEDFE
#define bright_white #E6E6E6
#define drac_background #282A36
#define drac_current_line #44475A
#define drac_foreground #F8F8F2
#define drac_comment #6272A4
#define drac_orange #FFB86C

*.foreground: drac_foreground
*.background: dracbackground

