! Dracula Xresources palette
#define black #000000
#define alt_gray #4D4D4D
#define drac_red #FF5555
#define alt_red #FF6E67
#define drac_green #50FA7B
#define drac_yellow #F1FA8C
#define drac_purple #BD93F9
#define drac_magenta #FF79C6
#define drac_cyan #8BE9FD
#define white #BFBFBF
#define alt_green #5AF78E
#define alt_yellow #F4F99D
#define alt_purple #CAA9FA
#define alt_magenta #FF92D0
#define alt_cyan #9AEDFE
#define alt_white #E6E6E6
#define drac_background #282A36
#define drac_current_line #44475A
#define drac_foreground #F8F8F2
#define drac_comment #6272A4

*.foreground: drac_foreground
*.background: dracbackground

! black
*.color0:     black
*.color8:     alt_gray

! red
*.color1:     drac_red
